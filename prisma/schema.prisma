generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Token {
    id         BigInt  @id @default(autoincrement())
    jti        String
    user_id    BigInt
    is_revoked Boolean
    createdAt  DateTime @default(now()) @db.Timestamptz(6)

    @@map(name: "tokens")
}
